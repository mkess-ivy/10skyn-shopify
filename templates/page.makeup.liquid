<div id="content">
  {% section 'makeup-banner' %}
</div>

<div id="content" class="row">
  {% section 'makeup-section-001' %}

  <div class="sk-product-wrapper">
    <div class="featured-collection-3col">
      <div class="frow justify-start">
        {% for link in linklists.iconic-makeup.links %}
        <div class="collection-thumb-3">
          <a href="{{ page.metafields.global.book-link }}">
            <img class="collection-thumb-img" src="{{ link.object.featured_image | img_url: 'master' }}" />
          </a>
          <div class="collection-thumb-title">
            <a href="{{ page.metafields.global.book-link }}" class="link-hover">{{ link.object.title | escape }}</a>
            <div class="collection-price">{{ link.object.price | money_without_trailing_zeros }}</div>
          </div>
          <div class="collection-thumb-desc">{{ link.object.description }}</div>
          <!-- <a href="{{ page.metafields.global.makeup-link }}" class="collection-thumb-link" target="_blank">
            <div>Book Now</div>
          </a> -->
        </div>
        {% endfor %}
      </div>
    </div>
  </div>
  
  {% section 'makeup-section-002' %}
  
