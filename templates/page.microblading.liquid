<div id="content">
  {% section 'microblading-banner' %}
</div>

<div id="content" class="row">
  {% section 'microblading-section' %}

  <div class="sk-product-wrapper">
    <div class="featured-collection-3col">
      <div class="frow justify-start">
        {% for link in linklists.microblading.links %}
        <div class="collection-thumb-3">
          <a href="{{ page.metafields.global.microblading-link }}"><img
            class="collection-thumb-img"
            src="{{ link.object.featured_image | img_url: 'master' }}"
          /></a>
          <div class="collection-thumb-title">
            <a href="{{ page.metafields.global.microblading-link }}" class="link-hover">{{ link.object.title | escape }}</a>
            <div class="collection-price">{{ link.object.price | money_without_trailing_zeros }}</div>
          </div>
          <div class="collection-thumb-desc">{{ link.object.description }}</div>
          
        </div>
        {% endfor %}
      </div>
    </div>
  </div>

  {% section 'microblading-alpha' %} 
  {% section 'microblading-ntk' %} 
  {% section 'microblading-faqs-001' %} 
  {% section 'microblading-faqs-002' %}
</div>
 
{% section 'component-book-cta-other' %}
