theme.mobileNav = (function() {
  function mobileNav(container) {
    (function accordion(container) {
      $('#accordion').find('.accordion-toggle').click(function(){
        //Expand or collapse this panel
        $(this).next().slideToggle('fast');
          $(this).attr('aria-expanded', function(index, attr){
            return attr == 'false' ? true : 'false';
          });
        //Hide the other panels
        $(".accordion-content").not($(this).next()).slideUp('fast');
      });

      $('#accordion').find('.accordion-toggle2').click(function(){
        //Expand or collapse this panel
        $(this).toggleClass('open');
        $(this).next().slideToggle('fast');
          $(this).attr('aria-expanded', function(index, attr){
            return attr == 'false' ? true : 'false';
          });
        //Hide the other panels
        $(".accordion-content2").not($(this).next()).slideUp('fast');
      });
    })();
    (function slideout() {
      var trigger = document.querySelector(".nav-trigger");
      var siteWrap = document.querySelector(".site-wrap");
      var hamburger = document.querySelector(".hamburger");

      trigger.addEventListener("click", menu);

      function menu() {
        var element = document.querySelector(".mobile-navigation");

        if (element.style.display === "none") {
          showMenu();
          return false;
        } else if (element.style.display === "block")  {
          hideMenu();
          return false;
        }

        function showMenu() {
          element.style.display = "block";
          siteWrap.classList.add("menu-open");
          hamburger.classList.add("is-active");
          trigger.setAttribute('aria-expanded', 'true');
        }
        function hideMenu() {
          siteWrap.classList.remove("menu-open");
          hamburger.classList.remove("is-active");

          setTimeout(hideMenu, 1200);
          function hideMenu() {
            element.style.display = "none";
            trigger.setAttribute('aria-expanded', 'false');
          }
        }
      }
    })();
  }
  mobileNav.prototype = _.assignIn({}, mobileNav.prototype, {
    onSelect: function(event) {
      var element = document.querySelector(".mobile-navigation");

        var siteWrap = document.querySelector(".site-wrap");
        var hamburger = document.querySelector(".hamburger");

        element.style.display = "block";
        siteWrap.classList.add("menu-open");
        hamburger.classList.add("is-active");
    },
    onDeselect: function(event) {
      var siteWrap = document.querySelector(".site-wrap");
      var hamburger = document.querySelector(".hamburger");
      var element = document.querySelector(".mobile-navigation");

      element.style.display = "none";
      siteWrap.classList.remove("menu-open");
      hamburger.classList.remove("is-active");
    }
  });

  return mobileNav;
})();

