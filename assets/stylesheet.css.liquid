/* Center Cells */
.gridlock .row [class*="desktop-"].center { margin-left: auto; margin-right: auto; }

/* !Generic, fractions */
.gridlock .row .desktop-third { width: 31.25%; }
.gridlock .row .desktop-half { width: 47.9166666666666667%; }
