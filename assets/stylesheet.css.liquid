/* #Base Styles
================================================== */

body {
  background: {{ settings.background }};
  font-family:{{ main_font.family }};
  font-weight: {{ main_font.weight }};
  font-size: {{ settings.font_size }}px;
  {% comment %} line-height: calc(({{ settings.font_size }}px * 0.375) + {{ settings.font_size }}px); {% endcomment %}
  text-shadow: 1px 1px 1px rgba(0,0,0,0.004);
  -webkit-text-size-adjust: 100%;
  -ms-text-size-adjust: 100%;
  color: {{ settings.text_color }};
}

.shifter-page {
  z-index: 1;
  position: relative;
  background: {{ settings.background }};
}

/* #Typography
================================================== */

h1, h2, h3, h4, h5, h6 {
  color: {{ settings.heading_color }};
  font-family:{{ heading_font.family }};
  font-weight: {{ heading_font.weight }};
  margin-top: 0;
  margin-bottom: 15px;
  line-height: normal;
}

h1 a, h2 a, h3 a, h4 a, h5 a, h6 a { font-weight: inherit; }
h1 {
  font-size: {{ settings.h1_size }}px;
  line-height: calc(({{ settings.h1_size }}px * 0.17) + {{ settings.h1_size }}px);
}
h2 {
  font-size: {{ settings.h2_size }}px;
  line-height: calc(({{ settings.h2_size }}px * 0.17) + {{ settings.h2_size }}px);
}
h3 {
  font-size: {{ settings.h3_size }}px;
  line-height: calc(({{ settings.h3_size }}px * 0.17) + {{ settings.h3_size }}px);
}
h4 {
  font-size: {{ settings.h4_size }}px;
  line-height: calc(({{ settings.h4_size }}px * 0.17) + {{ settings.h4_size }}px);
}
h5 {
  font-size: {{ settings.h5_size }}px;
  line-height: calc(({{ settings.h5_size }}px * 0.17) + {{ settings.h5_size }}px);
}
h6 {
  color: {{ settings.heading_color }};
  font-family:{{ heading_font.family }};
  font-weight: {{ heading_font.weight }};
  font-size: 18px;
  font-stretch: normal;
  font-style: normal;
  line-height: 1.2;
  letter-spacing: normal;
}
.section-title {
  {% if settings.section_title_border == 'top-and-bottom-full' %}
  border-top: {{ settings.heading_border_weight }}px solid {{ settings.dotted_color }};
  border-bottom: {{ settings.heading_border_weight }}px solid {{ settings.dotted_color }};
  padding-top: 10px;
  {% elsif settings.section_title_border == 'bottom-only-full' %}
  border-bottom: {{ settings.heading_border_weight }}px solid {{ settings.dotted_color }};
  {% elsif settings.section_title_border == 'none' %}
  {% endif %}
  padding-bottom: 10px;
  margin-bottom: 15px;
  text-align: center;
  position: relative;
}

{% if settings.section_title_border == 'bottom-only-short' %}
.section-title:after {
  content: "";
  width: 100px;
  margin-left: -50px;
  left: 50%;
  bottom: 3px;
  position: absolute;
  border-bottom: {{ settings.heading_border_weight }}px solid {{ settings.dotted_color }};
}
{% endif %}

p {
  font-size: {{ settings.font_size }}px;
  margin-bottom: 10px;
  line-height: calc(({{ settings.font_size }}px * 0.375) + {{ settings.font_size }}px);
}

